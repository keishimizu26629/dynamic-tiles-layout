{
  "dart.flutterSdkPath": ".fvm/versions/3.19.5",
  "dart.sdkPath": ".fvm/versions/3.19.5/bin/cache/dart-sdk",
  "search.exclude": {
    "**/.fvm": true,
    "**/*.freezed.dart": true,
    "**/*.g.dart": true,
    "**/.dart_tool": true,
    "**/*.mocks.dart": true,
    "**/*.gen.dart": true,
    "**/*.log": true,
    "**/*.flutterfire_gen.dart": true
  },
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "dart.runPubGetOnPubspecChanges": "never",
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.rulers": [
    80
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "quickfix.insertSemicolon": "explicit"
  },
  "cSpell.words": [
    "combinators",
    "lato",
    "linkify",
    "MEILISEARCH",
    "naoyuki",
    "todos",
    "wechat",
    "ZENTECH"
  ],
  "cSpell.ignorePaths": [
    "**/.vscode/**",
    "**/fvm/**"
  ]
}
